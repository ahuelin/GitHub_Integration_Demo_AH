{
  "app": {
    "base_language": "en",
    "push": {
      "source": [
        {
          "path": "./Properties/en/en_strings.properties",
          "file_format": "java_properties",
          "tag": "app:ApplicationMessages.properties",
          "language": "en"
        },
        {
          "path": "./CSV/en/table_en.csv",
          "file_format": "csv",
          "tag": "app:language.csv",
          "columnDescription": {
            "KEY": 0,
            "en": 1
          },
          "excludeHeaderRow": true,
          "language": "en"
        }
      ]
    },
    "pull": {
      "target": [
        {
          "path": "./Properties/en/<language>.properties",
          "file_format": "java_properties",
          "tag": "app:ApplicationMessages.properties",
          "exclude_languages": [
            "en"
          ]
        },
        {
          "path": "./CSV/en/<language>.csv",
          "file_format": "csv",
          "tag": "app:language.csv",
          "excludeHeaderRow": true,
          "excludeBaseLang": true,
          "columnDescription": {
            "KEY": 0,
            "<language>": 1
          },
          "exclude_languages": [
            "en"
          ]
        }
      ]
    }
  }
}