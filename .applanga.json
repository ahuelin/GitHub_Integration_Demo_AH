{
  "app": {
    "base_language": "en",
    "push": {
      "source": [
        {
          "path": "./Properties/en/en_strings.properties",
          "file_format": "java_properties",
          "tag": "messages",
          "language": "en"
        },
        {
          "path": "./ReactJSON/en/en.json",
          "file_format": "react_nested_json",
          "tag": "settings",
          "key_prefix": "v1",
          "language": "en"
        }
      ]
    },
    "pull": {
      "target": [
        {
          "path": "./Properties/en/<language>.properties",
          "file_format": "java_properties",
          "tag": "messages",
          "exclude_languages": [
            "en"
          ]
        },
        {
          "path": "./ReactJSON/en/<language>.json",
          "file_format": "react_nested_json",
          "tag": "settings",
          "key_prefix": "v1",
          "exclude_languages": [
            "en"
          ]
        }
      ]
    }
  }
}